{
  "name": "clean-dest",
  "version": "1.3.2",
  "description": "A CLI to clean a destination directory given a source directory",
  "bin": "./dist/bin.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src/**/*.ts",
    "test": "c8 --all mocha dist/**/*.spec.js",
    "watch-build": "tsc --watch",
    "watch-lint": "nodemon --watch ./src -e ts --exec npm run-script lint",
    "watch-test": "nodemon --watch ./dist -e js --exec npm run-script test",
    "release": "release-it",
    "docs": "jsdoc2md dist/index.js > API.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeanSobey/clean-dest.git"
  },
  "keywords": [
    "typescript",
    "clean"
  ],
  "author": "Sean Sobey",
  "license": "MIT",
  "bugs": "https://github.com/SeanSobey/markdown-preprocessor/issues",
  "homepage": "https://github.com/SeanSobey/clean-dest#readme",
  "dependencies": {
    "del": "^5.1.0",
    "globby": "^11.0.0",
    "trash": "^6.1.1",
    "tslib": "^1.10.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@schemastore/eslintrc": "0.0.5",
    "@types/globby": "^9.1.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.5.2",
    "@types/yargs": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "c8": "^7.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "release-it": "^12.4.3",
    "tslint": "^6.0.0",
    "tslint-divid": "^1.3.0",
    "typescript": "^3.7.5"
  }
}
